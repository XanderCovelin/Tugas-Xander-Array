#include <iostream>
using namespace std;

int main() {
    int angka[7];
    int maksimum;

    cout << "Masukkan 7 angka integer:" << endl;
    for (int i = 0; i < 7; i++) {
        cout << "Angka ke-" << (i + 1) << ": ";
        cin >> angka[i];
    }

    maksimum = angka[0];

    for (int i = 1; i < 7; i++) {
        if (angka[i] > maksimum) {
            maksimum = angka[i];
        }
    }

    cout << "\nNilai maksimum dalam array adalah: " << maksimum << endl;

    return 0;
}